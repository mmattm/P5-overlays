{
  "manifest_version": 3,
  "name": "P5 Overlays",
  "version": "1.0",
  "description": "Example of a layered canvas",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "p5.min.js",
        "p5.sound.min.js",
        "utils.js",
        "/sketches/example_headlines.js"
      ],
      "css": ["styles.css"]
    }
  ]
}
